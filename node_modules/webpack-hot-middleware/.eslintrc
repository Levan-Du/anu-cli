{
    "env": {
       "node": true
    },
    "rules": {
        "strict": 0,
        "quotes": 0,
        "curly": [2, "multi-line"],
        "no-use-before-define": [1, "nofunc"],
        "no-unused-vars": [2, "all"],
        "no-mixed-requires": [1, true],
        "max-depth": [1, 5],
        "max-len": [1, 80, 4],
        "eqeqeq": 0,
        "new-cap": 0,
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-lonely-if": 2,
        "no-path-concat": 0,
        "comma-dangle": 0,
        "camelcase": 0
    }
}
